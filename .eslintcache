p((user) => {\n        return user.username;\n      });\n      this.setState({ usernames, isLoading: false });\n    } catch (error) {\n      console.log(error);\n    }\n  }\n\n  handleChange(event) {\n    this.setState({\n      [event.target.name]: event.target.value,\n    });\n  }\n\n  handleUserSubmit(event) {\n    event.preventDefault();\n    const validUser = this.state.usernames.includes(this.state.username);\n    if (validUser) {\n      this.props.verifyUser(true, true, this.state.username);\n    }\n  }\n\n  async handleNewUserSubmit(event) {\n    event.preventDefault();\n    try {\n      const newUser = {\n        username: this.state.newUsername,\n        name: this.state.name,\n        avatar_url: this.state.avatar_url,\n      };\n      const data = await sendNewUser(newUser);\n      if (data.newUser.username) {\n        this.setState({ updateSuccess: true });\n      }\n\n      if (this.state.updateSuccess) {\n        this.props.verifyUser(true, true, this.state.newUsername);\n      }\n    } catch (error) {\n      console.log(error);\n    }\n  }\n\n  handleShowSignup(event) {\n    event.preventDefault();\n    this.setState({ showSignup: true });\n  }\n\n  render() {\n    if (this.state.isLoading) {\n      return <Loading />;\n    } else if (!this.state.showSignup) {\n      return (\n        <div className='loginContainer'>\n          <h1 className='loginTitle'>Login</h1>\n          <div className='existingUser'>\n            <form\n              className='loginEntry'\n              onSubmit={(event) => this.handleUserSubmit(event)}\n            >\n              <label htmlFor='username'>\n                Enter Username\n                <input\n                  type='text'\n                  id='username'\n                  name='username'\n                  value={this.state.username}\n                  onChange={(event) => this.handleChange(event)}\n                ></input>\n              </label>\n              <button className='loginButton' type='submit'>\n                {' '}\n                Login{' '}\n              </button>\n            </form>\n          </div>\n          <div className='showSignup'>\n            <button\n              className='showSignupButton'\n              onClick={(event) => this.handleShowSignup(event)}\n            >\n              New Users\n            </button>\n          </div>\n        </div>\n      );\n    } else {\n      return (\n        <div className='loginContainer'>\n          <h1 className='loginTitle'>Login</h1>\n          <div className='existingUser'>\n            <form\n              className='loginEntry'\n              onSubmit={(event) => this.handleUserSubmit(event)}\n            >\n              <label htmlFor='username'>\n                Enter Username\n                <input\n                  type='text'\n                  id='username'\n                  name='username'\n                  value={this.state.username}\n                  onChange={(event) => this.handleChange(event)}\n                ></input>\n              </label>\n              <button className='loginButton' type='submit'>\n                {' '}\n                Login{' '}\n              </button>\n            </form>\n          </div>\n          <div className='signup'>\n            <h1 className='signupTitle'>Signup</h1>\n            <div className='newUser'>\n              <form\n                className='signupEntry'\n                onSubmit={(event) => this.handleNewUserSubmit(event)}\n              >\n                <label htmlFor='username'>\n                  Enter New Username\n                  <input\n                    type='text'\n                    id='newUsername'\n                    name='newUsername'\n                    value={this.state.newUsername}\n                    onChange={(event) => this.handleChange(event)}\n                  ></input>\n                </label>\n                <label htmlFor='name'>\n                  Enter your name\n                  <input\n                    type='text'\n                    id='name'\n                    name='name'\n                    value={this.state.name}\n                    onChange={(event) => this.handleChange(event)}\n                  ></input>\n                </label>\n                <label htmlFor='avatar_url'>\n                  Enter your avatar URL\n                  <input\n                    type='text'\n                    id='avatar_url'\n                    name='avatar_url'\n                    value={this.state.avatar_url}\n                    onChange={(event) => this.handleChange(event)}\n                  ></input>\n                </label>\n                <button className='loginButton' type='submit'>\n                  {' '}\n                  Submit + Login{' '}\n                </button>\n              </form>\n            </div>\n          </div>\n        </div>\n      );\n    }\n  }\n}\n\nexport default Login;\n",["53","54"],"/Users/alanharrison/Desktop/Northcoders/frontend/nc-news-frontend/src/components/api.js",[],"/Users/alanharrison/Desktop/Northcoders/frontend/nc-news-frontend/src/components/loading.js",[],"/Users/alanharrison/Desktop/Northcoders/frontend/nc-news-frontend/src/components/header.js",[],"/Users/alanharrison/Desktop/Northcoders/frontend/nc-news-frontend/src/components/nav.js",["55","56"],"/Users/alanharrison/Desktop/Northcoders/frontend/nc-news-frontend/src/components/singleArticle.js",[],"/Users/alanharrison/Desktop/Northcoders/frontend/nc-news-frontend/src/components/articlesList.js",[],"/Users/alanharrison/Desktop/Northcoders/frontend/nc-news-frontend/src/components/addArticle.js",[],"/Users/alanharrison/Desktop/Northcoders/frontend/nc-news-frontend/src/components/home.js",[],{"ruleId":"57","severity":1,"message":"58","line":5,"column":8,"nodeType":"59","messageId":"60","endLine":5,"endColumn":13},{"ruleId":"57","severity":1,"message":"61","line":6,"column":10,"nodeType":"59","messageId":"60","endLine":6,"endColumn":